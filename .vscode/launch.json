    "version": "0.2.0",
    "configurations": [
        {
            "name": "SPA-Base: Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/spa_d",
            "cwd": "${workspaceFolder}",
            "env":{
                "SPA_BASE_BASE_URL": "/assets/",
                "SPA_BASE_ROOTS": "./cmd/spa_d/test/data/",
                "SPA_BASE_ALLOW_SKIP_BASE_URL": "true",

            }
        }
    ]
}